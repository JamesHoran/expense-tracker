generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model expenses {
  title     String
  amount    Decimal
  createdAt DateTime @default(now())
  id        String   @id(map: "Expense_pkey") @default(dbgenerated("gen_random_uuid()")) @db.Uuid
}
